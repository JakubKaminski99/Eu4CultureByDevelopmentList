pageE <- read_html("https://eu4.paradoxwikis.com/Economic_list_of_provinces")
tablesE <- pageE %>% html_table(fill=TRUE)
tableE <- tablesE[[1]]
pageP <- read_html("https://eu4.paradoxwikis.com/Political_list_of_provinces")
tablesP <- pageP %>% html_table(fill=TRUE)
tableP <- tablesP[[1]]
ecopol <- merge(tableE, tableP, by=c("ID"))
ecopol <- ecopol[c(3,11,13)]
culturePower <- aggregate(ecopol$Development, by=list(Category=ecopol$Culture), FUN=sum)
orderedCP <- culturePower[order(culturePower$x,decreasing = TRUE),]
